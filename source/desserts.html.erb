      <h2>Lefse</h2>
    </div>

    <div class="image col-md-6 web">
      <img class="img-fluid" src="<%= image_path('lefse_1.jpg') %>" />
      <img class="img-fluid" src="<%= image_path('lefse_2.jpg') %>" />
